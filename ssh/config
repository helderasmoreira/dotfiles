# movercado

Host *.movercado.org
    User movercado

# reedsy old infrastructure - staging

Host sinuous-axiom.staging.ssh-proxy.reedsy.com
  User deploy
  ForwardAgent yes
  IdentityFile ~/.ssh/reedsy

Host staging-*.c.sinuous-axiom-223012.internal
  User deploy
  IdentityFile ~/.ssh/reedsy
  ProxyCommand ssh -q -W %h:%p sinuous-axiom.staging.ssh-proxy.reedsy.com

# reedsy old infrastructure - production

Host sinuous-axiom.production.ssh-proxy.reedsy.com
  User deploy
  ForwardAgent yes
  IdentityFile ~/.ssh/reedsy

Host *.c.sinuous-axiom-223012.internal
  User deploy
  IdentityFile ~/.ssh/reedsy
  ProxyCommand ssh -q -W %h:%p sinuous-axiom.production.ssh-proxy.reedsy.com

